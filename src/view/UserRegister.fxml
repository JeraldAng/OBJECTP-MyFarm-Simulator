      <Button fx:id="btn_distinguished" blendMode="MULTIPLY" layoutX="95.0" layoutY="198.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="237.0" text="Distinguished (lvl 15): 250 coins">
