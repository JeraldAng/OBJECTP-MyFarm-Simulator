<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ServerView">
   <children>
      <ImageView fx:id="img_farmBG" fitHeight="400.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
      </ImageView>
      <Label layoutX="40.0" lineSpacing="1.0" prefHeight="126.0" prefWidth="320.0" text="MY FARM Simulator" textAlignment="CENTER" textFill="#0c7218" textOverrun="CLIP">
         <font>
            <Font name="Gameplay Regular" size="27.0" />
         </font>
      </Label>
      <Button fx:id="btn_play" blendMode="GREEN" defaultButton="true" layoutX="160.0" layoutY="273.0" mnemonicParsing="false" text="Play" textAlignment="CENTER">
         <font>
            <Font name="Gameplay Regular" size="19.0" />
         </font>
      </Button>
      <TextField fx:id="txt_name" layoutX="60.0" layoutY="175.0" prefHeight="50.0" prefWidth="282.0" promptText="Enter Username">
         <font>
            <Font name="Gameplay Regular" size="24.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
